{
  "outputDirectory": "build",
  "framework": "vite",
  "redirects": [
    {
      "source": "https://www.easyexcel.in/:path*",
      "destination": "https://easyexcel.in/:path*",
      "permanent": true
    },
    {
      "source": "http://www.easyexcel.in/:path*",
      "destination": "https://easyexcel.in/:path*",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

